<?xml version="1.0" encoding="UTF-8"?>

<look-and-feel>
	<compatibility>
		<version>6.2+</version>
	</compatibility>
	<theme id="anyplast" name="anyplast">
		<root-path>/</root-path>
		<templates-path>${root-path}/templates</templates-path>
		<images-path>${root-path}/images</images-path>
		<template-extension>vm</template-extension>
		<settings>
			<setting key="hello" value="world"></setting>
			<setting key="hi" value="mom"></setting>
		</settings>
		<roles>
			<role-name>User</role-name>
		</roles>
		<color-scheme id="01" name="Corporative">
			<css-class>blue</css-class>
			<color-scheme-images-path>${images-path}/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="02" name="Institutional">
			<css-class>green</css-class>
		</color-scheme>
		<color-scheme id="03" name="Alternative">
			<css-class>orange</css-class>
		</color-scheme>
	</theme>
</look-and-feel>